<% if current_user.level_id == 1 %>
<link type="text/stylesheet" href="assets/level1.css">
<% else %>
<% end %>
<div class="dashboard">
	    <button id = "play-sound">Play audio</button>
	    <button id = "stop-sound">Stop audio</button>


	<div class = "primary-block content-block">
			<div>Level</div>
			<div><%= current_user.level_id %></div>
	</div>

	<div class = "primary-block content-block">
